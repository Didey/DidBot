  var showNavBar = true
  $(window).on('scroll', function () {
    // 利用事例のページではフェードしない
    if ($('body').attr('class') === 'case') return

    var scrollAmount = $(window).scrollTop()
    if (scrollAmount === 0 && !showNavBar) {
      $('.navbar').css('opacity', 1)
      showNavBar = true
    }
    if (scrollAmount != 0 && showNavBar) {
      $('.navbar').animate({opacity: 0}, 'fast')
      showNavBar = false
    }
